<table class = "table table-striped">
  <tr>
    <th><% users.each do |user| %>
          <%= link_to user_path(user) do %>
            <%= attachment_image_tag(user, :profile_image, :fill, 50, 50, fallback: "no-image-icon.jpg") %><br>
            <%= user.name %>
    </th>
          <% end %>
        <% end %>
  </tr>
</table>